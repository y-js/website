{
  "private": true,
  "name": "yjs-website",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender && esdoc -c .esdoc.json",
    "serve": "npm run build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "node-sass": "^4.7.2",
    "preact-cli": "^2.0.0",
    "sass-loader": "^6.0.6",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-material-components": "^1.2.4",
    "preact-router": "^2.5.5",
    "quill": "^1.3.5",
    "y-websockets-client": "^9.0.0-4",
    "yjs": "^13.0.0-60"
  }
}
